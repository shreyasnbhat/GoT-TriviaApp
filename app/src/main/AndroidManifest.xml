<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shreyas.thrones">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".Firebase">
        <activity android:name="com.example.shreyas.thrones.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.shreyas.thrones.HouseActivity"
            android:parentActivityName="com.example.shreyas.thrones.MainActivity" />
        <activity android:name="com.example.shreyas.thrones.Characters"
                  android:parentActivityName="com.example.shreyas.thrones.MainActivity"
            />
    </application>

</manifest>